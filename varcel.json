{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://your-appwrite-server.com/"
    },
    {
      "src": "/sign-in",
      "dest": "https://your-appwrite-server.com/sign-in"
    }
  ]
}
